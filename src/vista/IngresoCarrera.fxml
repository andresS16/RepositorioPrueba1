<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlador.IngresoCarrera">
   <children>
      <VBox prefHeight="436.0" prefWidth="468.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
         <children>
            <TextField fx:id="txtNombre" promptText="Nombre">
               <VBox.margin>
                  <Insets bottom="10.0" left="75.0" right="75.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="comboCursada" prefWidth="179.0" promptText="Cursada">
               <VBox.margin>
                  <Insets bottom="10.0" left="75.0" right="75.0" top="20.0" />
               </VBox.margin>
            </ComboBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#guardarCarrera" prefHeight="25.0" prefWidth="114.0" text="Guardar">
                     <HBox.margin>
                        <Insets left="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnEliminar" mnemonicParsing="false" onAction="#actionEvent" prefHeight="25.0" prefWidth="118.0" text="Eliminar">
                     <HBox.margin>
                        <Insets left="5.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnModificar" mnemonicParsing="false" onAction="#actionEvent" prefHeight="25.0" prefWidth="117.0" text="Modificar">
                     <HBox.margin>
                        <Insets left="5.0" right="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
