<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="PanelInicioSesion" alignment="CENTER" prefHeight="411.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlador.Login2Controlador">
   <children>
      <Label text="Inisiar de Sesion">
         <VBox.margin>
            <Insets top="62.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <VBox>
         <VBox.margin>
            <Insets left="50.0" right="50.0" />
         </VBox.margin>
         <children>
            <Label text="Usuario">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txtUsuarioInSesion" prefHeight="37.0" promptText="Ej, admin">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label text="Contraseña">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <StackPane>
         <VBox.margin>
            <Insets left="50.0" right="50.0" />
         </VBox.margin>
         <children>
            <PasswordField fx:id="txtPasswordIniSesion" prefHeight="37.0" promptText="Ej,1234">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <TextField fx:id="txtPasswordIniSesionMask" prefHeight="37.0" promptText="Ej, 1234" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </StackPane>
      <CheckBox fx:id="chcMostrarContrseña" mnemonicParsing="false" text="Mostrar contraseña" textAlignment="CENTER" wrapText="true">
         <VBox.margin>
            <Insets left="-200.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </CheckBox>
      <HBox alignment="TOP_CENTER" prefWidth="200.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <children>
            <Button fx:id="btnIngresar" mnemonicParsing="false" onAction="#actionEvent" prefHeight="35.0" prefWidth="100.0" style="-fx-background-color: black;" text="Ingresar" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnLimpiar" mnemonicParsing="false" onAction="#actionEvent" prefHeight="35.0" prefWidth="100.0" style="-fx-background-color: #20c2bd;" text="Limpiar" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
